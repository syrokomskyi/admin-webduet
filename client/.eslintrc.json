{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "warn",
    // TODO stable Remove "@typescript-eslint/no-unsafe-call": "off".
    "@typescript-eslint/no-unsafe-call": "off",
    // TODO stable Remove "@typescript-eslint/no-unsafe-member-access": "off".
    "@typescript-eslint/no-unsafe-member-access": "off"
  }
}
